{{ if eq .chezmoi.os "linux" }}
{{   if eq .chezmoi.osRelease.id "ubuntu" }}
[global_config]
  title_hide_sizetext = True
  title_transmit_bg_color = "#43cc44"
  title_inactive_fg_color = "#deddda"
  title_inactive_bg_color = "#171421"
  inactive_color_offset = 0.3084745762711864
  enabled_plugins = LaunchpadBugURLHandler, LaunchpadCodeURLHandler, APTURLHandler, ActivityWatch
  suppress_multiple_term_dialog = True
  title_use_system_font = False
  title_font = JetBrains Mono NL Semi-Bold 13
[keybindings]
  split_horiz = <Primary><Shift>a
  split_vert = <Primary><Shift>d
  copy = <Primary><Shift>c
  paste = <Primary>v
[profiles]
  [[default]]
    audible_bell = True
    icon_bell = False
    background_color = "#1E1E2E"
    background_darkness = 0.93
    cursor_shape = ibeam
    cursor_color = "#a277ff"
    cursor_color_fg = False
    font = JetBrains Mono Bold 13
    foreground_color = "#eeeeee"
    show_titlebar = False
    palette = "#000000:#ff6e67:#57e389:#f4f99d:#caa8f9:#fe92d0:#9aedfd:#c7c7c7:#686868:#ff6e67:#5af78c:#f4f99d:#caa8f9:#fe92d0:#9aedfd:#ffffff"
    use_system_font = False
    bold_is_bright = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
{{   end }}
{{ end }}
